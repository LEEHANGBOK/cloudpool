                        <%
                          var google_state = drive_state.googleCount;
                          var dropbox_state = drive_state.dropboxCount;
                          var box_state = drive_state.boxCount
                        %>
                        <li><a href="#" id="dashboard"><span class='dashboard-ico nav-ico'></span><span>Dashboard</span></a></li>
                        <% if(google_state > 0) { %>
                        <li><a href="#" id="google"><span class='google-ico nav-ico'></span><span>Google</span></a></li>
                        <% } %>
                        <% if(dropbox_state > 0) { %>
                        <li><a href="#" id="dropbox"><span class='dropbox-ico nav-ico'></span><span>Dropbox</span></a></li>
                        <% } %>
                        <% if(box_state > 0) { %>
                        <li><a href="#" id="box"><span class='box-ico nav-ico'></span><span>Box</span></a></li>
                        <% } %>

                        <li><a href="#" class="setting-drive"><span class='connect-ico nav-ico'></span><span>드라이브 연결</span></a></li>


